{
    "name": "Prepo",
    "version": "0.0.1",
    "description": "Your companion for preparing for internships and placements",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "temp_logo.png"
    },
    "icons": {
        "128": "temp_logo.png"
    },
    "permissions": [
        "activeTab",
        "notifications",
        "tabs"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    }
}